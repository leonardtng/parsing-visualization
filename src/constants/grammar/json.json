{
  "start": "JSON",
  "productionMap": {
    "JSON": [["a", ["elem"]]],
    "elem": [["b", ["val"]]],
    "val": [
      ["c", ["obj"]],
      ["d", ["arr"]],
      ["p", ["str"]],
      ["f", ["num"]],
      ["g", ["bool"]],
      ["i", ["null"]]
    ],
    "obj": [
      ["k", ["{", "mems", "}"]],
      ["j", ["{", "}"]]
    ],
    "mems": [
      ["m", ["mem", ",", "mems"]],
      ["l", ["mem"]]
    ],
    "mem": [["n", ["str", ":", "elem"]]],
    "arr": [
      ["p", ["[", "elems", "]"]],
      ["o", ["[", "]"]]
    ],
    "elems": [
      ["r", ["elem", ",", "elems"]],
      ["q", ["elem"]]
    ],
    "str": [["s", ["\"", "chars", "\""]]],
    "chars": [
      ["u", ["char", "chars"]],
      ["e", []]
    ],
    "char": [
      ["w", ["esc"]],
      ["v", ["uni"]]
    ]
  },
  "directory": {
    "JSON": "JSON",
    "elem": "element",
    "val": "value",
    "obj": "object",
    "mems": "members",
    "mem": "member",
    "arr": "array",
    "elems": "elements",
    "str": "string",
    "chars": "characters",
    "char": "character",
    "esc": "escape",
    "uni": "unicode"
  },
  "whitespace": "\\s+",
  "terminalRules": [
    ["{", "\\{"],
    ["}", "\\}"],
    ["[", "\\["],
    ["]", "\\]"],
    [":", "\\:"],
    [",", "\\,"],
    ["\"", "\\\""],
    ["\\", "\\\\"],
    ["bool", "(true|false)"],
    ["null", "null"],
    ["esc", "(\\\\(\\\"|\\\\|/|b|f|n|r|t|u[0-9a-fA-F]{4}))"],
    ["num", "-?(0|[1-9]\\d*)(\\.\\d+)?([Ee][+-]?\\d+)?"],
    ["uni", "[\u0020-\u10FFFF&&[^\"\\]]"]
  ]
}
