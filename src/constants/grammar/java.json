{
  "start": "CD",
  "productionMap": {
    "CD": [["cd", ["AM", "class", "ID", "{", "CB", "}"]]],
    "AM": [
      ["am1", ["public"]],
      ["am2", ["private"]],
      ["am3", ["protected"]]
    ],
    "CB": [
      ["cb1", ["MD", "CB"]],
      ["cb2", []]
    ],
    "MD": [
      ["h", ["VD"]],
      ["i", ["MthD"]]
    ],
    "VD": [
      ["j1", ["Ty", "ID", ";"]],
      ["j2", ["Ty", "ID", "=", "L", ";"]]
    ],
    "MthD": [
      ["k", ["AM", "Ty", "ID", "(", "PL", ")", "{", "MB", "}"]],
      ["j", ["Ty", "ID", "(", "PL", ")", "{", "MB", "}"]]
    ],
    "PL": [
      ["l", ["P", ",", "PL"]],
      ["m", ["P"]],
      ["e", []]
    ],
    "P": [["o", ["Ty", "ID"]]],
    "MB": [
      ["p", ["MD", "MB"]],
      ["p", ["S", "MB"]],
      ["q", []]
    ],
    "S": [
      ["r", ["IfS"]],
      ["s", ["WhS"]],
      ["t", ["RtS"]],
      ["u", ["E", ";"]]
    ],
    "IfS": [
      ["v", ["if", "(", "E", ")", "S"]],
      ["w", ["if", "(", "E", ")", "{", "MB", "}", "ElseS"]],
      ["x", ["if", "(", "E", ")", "S", "ElseS"]]
    ],
    "ElseS": [
      ["y", ["else", "S"]],
      ["z", ["else", "{", "MB", "}"]],
      ["aa", ["else", "IfS"]],
      ["aa", []]
    ],
    "WhS": [["w", ["while", "(", "E", ")", "S"]]],
    "RtS": [["x", ["return", "E", ";"]]],
    "E": [
      ["y", ["ID", "=", "E"]],
      ["z", ["L"]],
      ["aa", ["ID"]]
    ],
    "Ty": [
      ["ab", ["int"]],
      ["ac", ["boolean"]],
      ["ad", ["void"]],
      ["ae", ["char"]],
      ["af", ["ID"]]
    ],
    "L": [
      ["af", ["NL"]],
      ["ag", ["SL"]],
      ["ah", ["BL"]]
    ],
    "ID": [["ai", ["chars"]]],
    "NL": [["aj", ["num"]]],
    "SL": [["ak", ["\"", "chars", "\""]]],
    "BL": [["al", ["bool"]]]
  },
  "directory": {
    "CD": "ClassDeclaration",
    "AM": "AccessModifier",
    "CB": "ClassBody",
    "MD": "MemberDeclaration",
    "VD": "VariableDeclaration",
    "MthD": "MethodDeclaration",
    "PL": "ParameterList",
    "P": "Parameter",
    "MB": "MethodBody",
    "S": "Statement",
    "IfS": "IfStatement",
    "ElseS": "ElseStatement",
    "WhS": "WhileStatement",
    "RtS": "ReturnStatement",
    "E": "Expression",
    "Ty": "Type",
    "L": "Literal",
    "ID": "Identifier",
    "NL": "NumberLiteral",
    "SL": "StringLiteral",
    "BL": "BooleanLiteral"
  },
  "whitespace": "\\s+",
  "terminalRules": [
    ["=", "\\="],
    ["{", "\\{"],
    ["}", "\\}"],
    ["[", "\\["],
    ["]", "\\]"],
    ["(", "\\("],
    [")", "\\)"],
    [":", "\\:"],
    [",", "\\,"],
    [";", "\\;"],
    ["\"", "\\\""],
    ["\\", "\\\\"],
    ["public", "public"],
    ["private", "private"],
    ["protected", "protected"],
    ["int", "int"],
    ["void", "void"],
    ["char", "char"],
    ["boolean", "boolean"],
    ["class", "class"],
    ["bool", "(true|false)"],
    ["if", "if"],
    ["else", "else"],
    ["while", "while"],
    ["return", "return"],
    ["num", "-?(0|[1-9]\\d*)(\\.\\d+)?([Ee][+-]?\\d+)?"],
    ["chars", "[a-zA-Z_][a-zA-Z0-9_]*"]
  ]
}
